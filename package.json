{
  "name": "navalivainext-twa",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "watch": "node watch.js",
    "build:server": "cd server && npm install && npm run build",
    "build:all": "npm run copy:types && concurrently \"npm run build:server\" \"npm run build\"",
    "copy:types": "mkdir -p server/src/types && cp -r shared/types/* server/src/types/ && cp -r server/src/types/* shared/types/ && cp -r shared/types/* src/types/",
    "vercel-build": "npm run build:all"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^5.10.0",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.7",
    "critters": "^0.0.23",
    "framer-motion": "^11.0.3",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "zod": "^3.22.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "prisma": "^5.10.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
